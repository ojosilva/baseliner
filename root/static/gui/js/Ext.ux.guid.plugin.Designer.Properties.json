    name : "enableHdMenu",
    type : "boolean",
    desc : "<b>Boolean</b> True to enable the drop down button for menu in the headers."
  },{
    name : "enableRowHeightSync",
    type : "boolean",
    desc : "<b>Boolean</b> True to manually sync row heights across locked and not locked rows."
  },{
    name : "stripeRows",
    type : "boolean",
    desc : "<b>Boolean</b> True to stripe the rows. Default is true."
