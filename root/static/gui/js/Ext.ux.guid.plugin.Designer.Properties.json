    name : "startCollapsed",
    type : "boolean",
    desc : "<b>Boolean</b> True to start all groups collapsed"
  },{
    name : "enableGroupingMenu",
    type : "boolean",
    desc : "<b>Boolean</b> True to enable the grouping control in the column menu"
