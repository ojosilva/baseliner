    name : "cm",
    type : "mixed",
    desc : "<b>Store</b> An Ext.grid.ColumnModel for this grid"
  },{
    name : "sm",
    type : "object",
    desc : "<b>Object</b> The SelectionModel the grid should use to handle selections (shortcut of selModel)"
  },{
    name : "selModel",
    type : "object",
    desc : "<b>Object</b> The SelectionModel the grid should use to handle selections"
  },{
    name : "minColumnWidth",
    type : "number",
    desc : "<b>Number</b> The minimum width a column can be resized to. Defaults to 25."
  },{
    name : "monitorWindowResize",
    type : "boolean",
    desc : "<b>Boolean</b> True to autoSize the grid when the window resizes. Defaults to true."
  },{
    name : "maxRowsToMeasure",
    type : "boolean",
    desc : "<b>Boolean</b> If autoSizeColumns is on, maxRowsToMeasure can be used to limit the number of rows measured to get a columns size - defaults to 0 (all rows)."
  },{
    name : "trackMouseOver",
    type : "boolean",
    desc : "<b>Boolean</b> True to highlight rows when the mouse is over. Default is false."
  },{
    name : "enableDragDrop",
    type : "boolean",
    desc : "<b>Boolean</b> True to enable drag and drop of rows."
