    name : "maxHeight",
    type : "number",
    desc : "<b>Number</b> Sets the maximum height - ignored if autoHeight is not on."
  },{
    name : "enableRowBody",
    type : "boolean",
    desc : "<b>Boolean</b> True to add a second TR element per row that can be used to provide a row body that spans beneath the data row. Use the {@link #getRowClass} method's rowParams config to customize the row body."
  },{
    name : "autoFill",
    type : "boolean",
    desc : "<b>Boolean</b> True to auto expand the columns to fit the grid <b>when the grid is created</b>."
  },{
    name : "forceFit",
    type : "boolean",
    desc : "<b>Boolean</b> True to auto expand/contract the size of the columns to fit the grid width and prevent horizontal scrolling."
  },{
    name : "hideGroupedColumn",
    type : "boolean",
    desc : "<b>Boolean</b> True to hide the column that is currently grouped"
