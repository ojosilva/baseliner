  },{
    name : "name",
    desc : "<b>String</b> The field's HTML name attribute."
  },{
    name : "checkboxToggle",
    type : "boolean",
    desc : "<b>Boolean</b> True to render a checkbox into the fieldset frame just in front of the legend (defaults to false). The fieldset will be expanded or collapsed when the checkbox is toggled."
  },{
    name : "checkboxName",
    desc : "<b>String</b> The name to assign to the fieldset's checkbox if {@link #checkboxToggle} = true (defaults to '[checkbox id]-checkbox')."
