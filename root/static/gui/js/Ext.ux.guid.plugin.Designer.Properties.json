  },{
    name : "align",
    desc : "<b>String</b> (Optional) Set the CSS text-align property of the column. Defaults to undefined."
  },{
    name : "clicksToEdit",
    type : "number",
    desc : "<b>Number</b> The number of clicks on a cell required to display the cell's editor (defaults to 2)"
