    name : "autoExpandColumn",
    desc : "<b>String</b> The id of a column in this grid that should expand to fill unused space. This id can not be 0."
  },{
    name : "autoExpandMin",
    type : "number",
    desc : "<b>Number</b> The minimum width the autoExpandColumn can have (if enabled). defaults to 50."
